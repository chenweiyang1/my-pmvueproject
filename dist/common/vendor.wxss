
.header {
  width: 100%;
  padding: 0 24rpx;
  height: 150rpx;
  position: fixed;
  top: 0;
  left: 0;
  background: -webkit-linear-gradient(left, #FF4A46 , #FF603D);
  background: -o-linear-gradient(right, #FF4A46, #FF603D);
  background: -moz-linear-gradient(right, #FF4A46, #FF603D);
  background: linear-gradient(to right, #FF4A46 , #FF603D);
  z-index: 9999;
}
.header .back-btn {
  width: 60rpx;
  height: 100%;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 22rpx auto;
  background-position: 0 70rpx;
  position: relative;
  z-index: 9;
}
.header .flex1 {
  padding-right: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 36rpx;
  color: #fff;
  line-height: 180rpx;
  position: relative;
  z-index: 9;
}
.header .header ._img {
  width: 130rpx;
  position: absolute;
  bottom: 0;
  left: 24rpx;
  height: 90rpx;
}
.header .headerbgbox {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 130rpx 90rpx;
  width: 130rpx;
  position: absolute;
  bottom: 0;
  left: 24rpx;
  height: 90rpx;
}