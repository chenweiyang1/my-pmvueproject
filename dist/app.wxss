@import "common/vendor.wxss";

view,
text,
._input,
._textarea,
._button,
._label,
swiper-item {
  font-family: "Microsoft YaHei";
  box-sizing: border-box;
}
page {
  background-color: #f6f5f5;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding-top: 150rpx;
  box-sizing: border-box;
}
.flex {
  display: flex;
}
.flex.flex-row {
  flex-direction: row;
}
.flex .flex1 {
  flex: 1;
  width: 20%;
}