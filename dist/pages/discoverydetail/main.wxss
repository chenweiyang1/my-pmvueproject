@import "../../common/vendor.wxss";

.discoverydetail {
  padding-bottom: 110rpx;
}
.discoverydetail .dynamic-detail {
  width: 100%;
  padding: 24rpx;
  background-color: #fff;
  box-shadow: 0 0 12rpx 0 rgba(0, 0, 0, 0.1);
}
.discoverydetail .dynamic-detail .user-line {
  width: 100%;
}
.discoverydetail .dynamic-detail .user-line .avatar {
  flex: 0 0 70rpx;
  height: 70rpx;
  overflow: hidden;
}
.discoverydetail .dynamic-detail .user-line .avatar ._img {
  width: 100%;
}
.discoverydetail .dynamic-detail .user-line .user-name {
  padding-left: 26rpx;
}
.discoverydetail .dynamic-detail .user-line .user-name .name {
  font-size: 28rpx;
  color: #1E1E1E;
  line-height: 30rpx;
  margin-bottom: 10rpx;
}
.discoverydetail .dynamic-detail .user-line .user-name .signature {
  font-size: 24rpx;
  color: #909090;
  line-height: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.discoverydetail .dynamic-detail .dynamic-content {
  margin-top: 24rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap .image {
  flex: 1;
  overflow: hidden;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap .image image {
  width: 100%;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.single .image {
  flex: 0 0 480rpx;
  max-height: 360rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.double .image {
  max-height: 240rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.double .image:first-child {
  padding-right: 3rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.double .image:last-child {
  padding-left: 3rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.three .image {
  max-height: 170rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.three .image:first-child {
  padding-right: 4rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.three .image:nth-child(2) {
  padding: 0 2rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .image-wrap.three .image:last-child {
  padding-left: 4rpx;
}
.discoverydetail .dynamic-detail .dynamic-content .text-wrap {
  font-size: 30rpx;
  color: #1E1E1E;
  line-height: 40rpx;
  margin-top: 20rpx;
}
.discoverydetail .dynamic-detail .like-comment-share {
  margin-top: 18rpx;
  border-top: 1rpx solid #EEEEEE;
  padding-top: 20rpx;
}
.discoverydetail .dynamic-detail .like-comment-share .praise {
  margin-right: 100rpx;
}
.discoverydetail .dynamic-detail .like-comment-share .praise .icon {
  display: inline-block;
  width: 28rpx;
  height: 26rpx;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 30rpx auto;
  background-image: url(data:image/png;base64,);
}
.discoverydetail .dynamic-detail .like-comment-share .praise .icon.clicked {
  animation: likeAnim 0.5s 1 forwards;
  background-image: url(data:image/png;base64,);
}
.discoverydetail .dynamic-detail .like-comment-share .praise.liked .icon {
  background-image: url(data:image/png;base64,);
}
.discoverydetail .dynamic-detail .like-comment-share .comment .icon {
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 27rpx auto;
  background-image: url(data:image/png;base64,);
}
.discoverydetail .dynamic-detail .like-comment-share .number {
  margin-left: 16rpx;
  font-size: 26rpx;
  color: #909090;
  line-height: 26rpx;
  position: relative;
  top: -2rpx;
}
.discoverydetail .comment-list {
  width: 100%;
  background-color: #fff;
  margin-top: 21rpx;
  box-shadow: 0 0 12rpx 0 rgba(0, 0, 0, 0.1);
}
.discoverydetail .comment-list .title {
  padding: 24rpx;
  font-size: 28rpx;
  color: #1E1E1E;
  line-height: 30rpx;
  position: relative;
}
.discoverydetail .comment-list .title .look-all {
  font-size: 26rpx;
  color: #909090;
  line-height: 30rpx;
}
.discoverydetail .comment-list .title .look-all .i {
  display: inline-block;
  width: 20rpx;
  height: 30rpx;
  background-position: right center;
  background-repeat: no-repeat;
  background-size: 14rpx auto;
  background-image: url(data:image/png;base64,);
  vertical-align: bottom;
  position: relative;
  top: 3rpx;
}
.discoverydetail .comment-list .title:after {
  content: '';
  display: block;
  width: calc(100% - 48rpx);
  height: 1rpx;
  background-color: #E8E8E8;
  position: absolute;
  left: 24rpx;
  bottom: 1rpx;
}
.discoverydetail .input-box {
  width: 100%;
  height: 110rpx;
  padding-right: 10rpx;
  box-shadow: 0 -12rpx 16rpx 0 rgba(8, 8, 8, 0.04);
  background-color: #fff;
  bottom: 0;
  left: 0;
  position: fixed;
}
.discoverydetail .input-box .to-home {
  flex: 0 0 100rpx;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 45rpx auto;
  height: 100%;
}
.discoverydetail .input-box .input {
  flex: 1;
  height: 100%;
  margin-right: 5rpx;
  padding: 20rpx;
}
.discoverydetail .input-box .input ._input {
  width: 100%;
  height: 100%;
  border: none;
  background-color: #eee;
  border-radius: 4rpx;
  padding: 0 10rpx;
  font-size: 30rpx;
  color: #1E1E1E;
}
.discoverydetail .input-box .like {
  flex: 0 0 80rpx;
  height: 100%;
  background-repeat: no-repeat;
  background-size: 43rpx auto;
  background-position: center;
  background-image: url(data:image/png;base64,);
  transition: all 0.3s;
}
.discoverydetail .input-box .like.liked {
  background-image: url(data:image/png;base64,);
}
.discoverydetail .input-box .share {
  flex: 0 0 80rpx;
  height: 100%;
  background-repeat: no-repeat;
  background-size: 34rpx auto;
  background-position: center;
  background-image: url(data:image/png;base64,);
}