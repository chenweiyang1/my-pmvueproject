@import "../../common/vendor.wxss";

@keyframes likeAnim {
0% {
    transform: scale(1.3);
}
100% {
    transform: scale(1);
}
}
.discovery {
  padding-top: 220rpx;
}
.discovery .img-move-bottom .headerbgbox {
  bottom: -70rpx;
}
.discovery .top-tab {
  width: 100%;
  height: 70rpx;
  background: linear-gradient(to right, #FF4A46, #FF603D);
  position: fixed;
  z-index: 99;
  left: 0;
  top: 150rpx;
}
.discovery .top-tab .menu-wrap {
  height: 100%;
  line-height: 70rpx;
  width: 270rpx;
  margin: auto;
}
.discovery .top-tab .menu-wrap .items {
  text-align: center;
  color: #fff;
  opacity: 0.3;
  font-size: 30rpx;
  transition: all 0.3s;
}
.discovery .top-tab .menu-wrap .items.active {
  position: relative;
  color: #fff;
  opacity: 1;
}
.discovery .top-tab .menu-wrap .items.active::after {
  content: '';
  display: block;
  width: 60rpx;
  height: 3rpx;
  border-radius: 3rpx;
  background-color: #fff;
  position: absolute;
  bottom: 3rpx;
  left: 50%;
  transform: translate(-50%, 0);
}
.discovery .list {
  width: 100%;
  padding: 24rpx;
}
.discovery .list .list-card {
  background-color: #fff;
  border-radius: 10rpx;
  padding: 24rpx;
  box-shadow: 0 0 12rpx rgba(0, 0, 0, 0.1);
  margin-bottom: 20rpx;
}
.discovery .list .list-card .user {
  width: 100%;
  height: 72rpx;
}
.discovery .list .list-card .user .avatar {
  flex: 0 0 72rpx;
  height: 72rpx;
  overflow: hidden;
}
.discovery .list .list-card .user .avatar image {
  width: 100%;
}
.discovery .list .list-card .user .user-info {
  padding-left: 24rpx;
}
.discovery .list .list-card .user .user-info .user-name {
  font-size: 28rpx;
  line-height: 30rpx;
}
.discovery .list .list-card .user .user-info .user-name .owner-name {
  color: #1E1E1E;
}
.discovery .list .list-card .user .user-info .user-name .pet-name {
  margin-left: 5rpx;
  color: #FF4E44;
}
.discovery .list .list-card .user .user-info .signature {
  font-size: 24rpx;
  color: #909090;
  line-height: 24rpx;
  margin-top: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.discovery .list .list-card .user .attention-status {
  flex: 0 0 126rpx;
  height: 52rpx;
  position: relative;
  top: 10rpx;
  border: 1rpx solid #DCDCDC;
  text-align: center;
  line-height: 50rpx;
  color: #FF5D3E;
  font-size: 28rpx;
  border-radius: 6rpx;
}
.discovery .list .list-card .user .attention-status .icon {
  display: inline-block;
  width: 28rpx;
  height: 100%;
  vertical-align: top;
  background-repeat: no-repeat;
  background-size: 24rpx auto;
  background-position: center;
  background-image: url(data:image/png;base64,);
}
.discovery .list .list-card .user .attention-status .text {
  position: relative;
  top: -2rpx;
}
.discovery .list .list-card .user .attention-status.followed {
  border-style: dashed;
  color: #C8C8C8;
}
.discovery .list .list-card .user .attention-status.followed .icon {
  display: none;
}
.discovery .list .list-card .card-content {
  margin-top: 24rpx;
}
.discovery .list .list-card .card-content .image-wrap .image {
  flex: 1;
  overflow: hidden;
}
.discovery .list .list-card .card-content .image-wrap .image image {
  width: 100%;
}
.discovery .list .list-card .card-content .image-wrap.single .image {
  flex: 0 0 480rpx;
  max-height: 360rpx;
}
.discovery .list .list-card .card-content .image-wrap.double .image {
  max-height: 240rpx;
}
.discovery .list .list-card .card-content .image-wrap.double .image:first-child {
  padding-right: 3rpx;
}
.discovery .list .list-card .card-content .image-wrap.double .image:last-child {
  padding-left: 3rpx;
}
.discovery .list .list-card .card-content .image-wrap.three .image {
  max-height: 170rpx;
}
.discovery .list .list-card .card-content .image-wrap.three .image:first-child {
  padding-right: 4rpx;
}
.discovery .list .list-card .card-content .image-wrap.three .image:nth-child(2) {
  padding: 0 2rpx;
}
.discovery .list .list-card .card-content .image-wrap.three .image:last-child {
  padding-left: 4rpx;
}
.discovery .list .list-card .card-content .text-wrap {
  font-size: 30rpx;
  color: #1E1E1E;
  line-height: 40rpx;
  margin-top: 20rpx;
}
.discovery .list .list-card .card-actions {
  margin-top: 18rpx;
  border-top: 1rpx solid #EEEEEE;
  padding-top: 20rpx;
}
.discovery .list .list-card .card-actions .praise {
  margin-right: 100rpx;
}
.discovery .list .list-card .card-actions .praise .icon {
  display: inline-block;
  width: 28rpx;
  height: 26rpx;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 30rpx auto;
  background-image: url(data:image/png;base64,);
}
.discovery .list .list-card .card-actions .praise .icon.clicked {
  animation: likeAnim 0.5s 1 forwards;
  background-image: url(data:image/png;base64,);
}
.discovery .list .list-card .card-actions .praise.liked .icon {
  background-image: url(data:image/png;base64,);
}
.discovery .list .list-card .card-actions .comment .icon {
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  background-repeat: no-repeat;
  background-position: left center;
  background-size: 27rpx auto;
  background-image: url(data:image/png;base64,);
}
.discovery .list .list-card .card-actions .number {
  margin-left: 16rpx;
  font-size: 26rpx;
  color: #909090;
  line-height: 26rpx;
  position: relative;
  top: -2rpx;
}